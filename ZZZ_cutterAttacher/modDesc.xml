<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<modDesc descVersion="21">
	<author>Mogli aka Biedens</author>
	<title>
		<en>Automatic height control of attached cutters</en>
		<de>Automatische Schneidwerkshöhenkontrolle</de>
		<pl>Automatyczna kontorla wysokości podczepionych hederów</pl>
	</title>
	<description>
		<en>Automatic height control of attached cutters
		</en>
		<de>Automatische Schneidwerkshöhenkontrolle
		</de>
		<pl>Automatyczna kontorla wysokości podczepionych hederów</pl>
	</description>
	<multiplayer supported="true"/>   
	<iconFilename>store.dds</iconFilename>
	<version>1.1.0.0</version>
	<l10n>
		<text name="ZZZ_CAH_VERSION">
			<en>cutter height control v1.1</en>
			<de>Schneidwerkshöhenkontrolle v1.1</de>
			<pl>Kontorla wysokości hederu v1.1</pl>
		</text>		
		<text name="ZZZ_CAH_OFF">
			<en>cutter height [normal]</en>
			<de>Schneidwerkshöhe [normal]</de>
			<pl>Wysokość hederu [normalna]</pl>
		</text>		
		<text name="ZZZ_CAH_ON">
			<en>cutter height [auto]</en>
			<de>Schneidwerkshöhe [auto]</de>
			<pl>Wysokość hederu [auto]</pl>
		</text>		
		<text name="ZZZ_CAH_DOWN">
			<en>cutter down (%0.2f)</en>
			<de>Schneidwerk tiefer (%0.2f)</de>
			<pl>Heder w dół (%0.2f)</pl>
		</text>		
		<text name="ZZZ_CAH_UP">
			<en>cutter up (%0.2f)</en>
			<de>Schneidwerk höher (%0.2f)</de>
			<pl>Heder w górę (%0.2f)</pl>
		</text>		
	</l10n>
	<inputBindings>		
		<input name="ZZZ_CAH_UP"            category="VEHICLE" key1="KEY_lshift KEY_j" key2="" mouse="" button="" device="0" />
		<input name="ZZZ_CAH_DOWN"          category="VEHICLE" key1="KEY_lshift KEY_n" key2="" mouse="" button="" device="0" />
		<input name="ZZZ_CAH_TOGGLE"        category="VEHICLE" key1="KEY_lshift KEY_h" key2="" mouse="" button="" device="0" visible="false" />
	</inputBindings>

	<extraSourceFiles>
		<sourceFile filename="register.lua" />
	</extraSourceFiles>    
</modDesc>
